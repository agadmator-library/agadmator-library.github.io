{
  "name": "agadmator-database.github.io",
  "version": "1.0.0",
  "description": "",
  "main": "build_scripts/loadNewMovies.ts",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "build-only": "mkdir -p generated && tsc --project ./tsconfig.generate.json && node build_scripts/nodeActions/generate.js && vite build && cp -r generated dist && cp -r img dist && cp -r js dist && cp -r db dist && cp -r css dist",
    "type-check": "vue-tsc --noEmit -p tsconfig.app.json --composite false",
    "loadNewVideos": "node --experimental-wasm-threads --experimental-wasm-simd build_scripts/nodeActions/loadNewVideos.js",
    "refreshVideo": "tsc --project ./tsconfig.generate.json && node build_scripts/nodeActions/refreshVideo.js",
    "loadMissingData": "tsc --project ./tsconfig.generate.json && node build_scripts/nodeActions/loadMissingData.js",
    "retryDataRetrieval": "tsc --project ./tsconfig.generate.json && node build_scripts/nodeActions/retryDataRetrieval.js",
    "extractGames": "tsc --project ./tsconfig.generate.json && node build_scripts/nodeActions/extractPgns.js",
    "generate": "tsc --project ./tsconfig.generate.json && node build_scripts/nodeActions/generate.js",
    "removeVideo": "tsc --project ./tsconfig.generate.json && node build_scripts/nodeActions/removeVideo.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "Mateusz Dziurdziak",
  "license": "Apache-2.0",
  "dependencies": {
    "@popperjs/core": "^2.11.8",
    "bootstrap": "^5.3.2",
    "pinia": "^2.1.6",
    "primevue": "^3.40.1",
    "vue": "^3.3.4",
    "vue-router": "^4.2.4"
  },
  "devDependencies": {
    "@tsconfig/node18": "^18.2.2",
    "@types/lodash": "^4.14.197",
    "@types/node": "^18.17.17",
    "@types/object-hash": "^3.0.3",
    "@vitejs/plugin-vue": "^4.3.4",
    "@vue/tsconfig": "^0.4.0",
    "axios": "^1.4.0",
    "cheerio": "^1.0.0-rc.12",
    "googleapis": "^126.0.0",
    "kokopu": "^4.7.0",
    "levenshtein-edit-distance": "^3.0.1",
    "lodash": "^4.17.21",
    "node-fetch": "^3.3.1",
    "npm-run-all2": "^6.0.6",
    "stockfish": "^15.0.0",
    "tinyduration": "^3.3.0",
    "ts-node": "^10.9.1",
    "typescript": "~5.2.0",
    "vite": "^4.4.9",
    "vue-tsc": "^1.8.11"
  }
}
